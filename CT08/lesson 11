char = "a"
key = 5

ascii_start = 32
ascii_end = 176

range_size = ascii_end - ascii_start + 1

if ascii_start <= ord(char) <= ascii_end:
    char_code = ord(char)
    print(char_code)

    shifted_code = char_code - ascii_start + key

    wrapped_code = shifted_code & range_size

    final_code = wrapped_code + ascii_start


char = chr(final_code)

print(char)