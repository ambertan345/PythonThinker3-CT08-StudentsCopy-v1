

def caesar_shfit_character(char, key, mode):

    ascii_start = 32
    ascii_end = 126
    range_size = ascii_end - ascii_start + 1



if ascii_start <= ord(char) <= ascii_end:
    char_code = ord(char)
    
    if mode == "encrypt":
        shifted_code = char_code - ascii_start + key
    elif mode == "decrypt":
        shifted_code = char_code - ascii_start - key
    else:
        raise ValueError ("Invalid mode")
    
